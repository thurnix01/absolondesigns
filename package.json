{
  "name": "portfolio-redesign",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:clean": "kill -9 $(lsof -ti:3000) 2>/dev/null || true && npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build",
    "deploy": "rm -rf node_modules/.cache .next out && next build && touch out/.nojekyll && echo '' > out/.nojekyll && git add out/ && git commit -m \"Deploy to GitHub Pages\" && git push origin `git subtree split --prefix out main`:gh-pages --force"
  },
  "dependencies": {
    "next": "^15.5.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "gh-pages": "^6.3.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
